{
    "name": "@fly-lab/eslint-config",
    "version": "0.0.0",
    "author": "DHN Chandan",
    "license": "MIT",
    "main": "index.js",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fly-lab/eslint-config.git"
    },
    "bugs": {
        "email": "fly.dev.org@gmail.com",
        "url": "https://github.com/fly-lab/eslint-config/issues"
    },
    "homepage": "https://github.com/fly-lab/eslint-config.git",
    "keywords": [
        "eslint",
        "eslintconfig",
        "typescript",
        "lint",
        "monorepo"
    ],
    "scripts": {
        "publish:np": "np --branch=master --message=\"chore: :bookmark: Publish v%s tag\"",
        "publish:np:false": "np --branch=master --message=\"chore: :bookmark: Publish v%s tag\" --publish=false",
        "publish:npm": "npm publish --access public"
    },
    "dependencies": {
        "@rushstack/eslint-config": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint-config-next": "^13.2.4",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-turbo": "^0.0.10",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-testing-library": "^5.10.2"
    },
    "peerDependencies": {
        "eslint": ">=8",
        "typescript": ">=4.7.0",
        "prettier": ">=4.0.0"
    },
    "peerDependenciesMeta": {
        "eslint": {
            "optional": false
        }
    },
    "devDependencies": {
        "eslint": "^8.36.0",
        "typescript": "^5.0.2"
    }
}