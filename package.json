{
    "name": "@fly-lab/eslint-config",
    "version": "1.2.2",
    "author": "DHN Chandan",
    "license": "MIT",
    "files": [
        "base/*",
        "node/*",
        "react/*",
        "prettier-config.json"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fly-lab/eslint-config.git"
    },
    "bugs": {
        "email": "fly.dev.org@gmail.com",
        "url": "https://github.com/fly-lab/eslint-config/issues"
    },
    "homepage": "https://github.com/fly-lab/eslint-config.git",
    "keywords": [
        "eslint",
        "eslintconfig",
        "typescript",
        "lint",
        "monorepo"
    ],
    "scripts": {
        "publish:np": "np --branch=master --message=\"chore: :bookmark: Publish v%s tag\"",
        "publish:np:false": "np --branch=master --message=\"chore: :bookmark: Publish v%s tag\" --publish=false",
        "publish:npm": "npm publish --access public",
        "changeset": "npx changeset",
        "changeset:status": "npx changeset status --verbose",
        "changeset:version": "npx changeset version",
        "changeset:publish": "npx changeset publish"
    },
    "dependencies": {
        "@rushstack/eslint-config": "^3.4.1",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "eslint-config-next": "^14.0.2",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-turbo": "^1.10.16",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jest": "^27.6.0",
        "eslint-plugin-jest-dom": "^5.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-testing-library": "^6.1.2"
    },
    "peerDependencies": {
        "eslint": ">=8.0.0",
        "prettier": ">=3.0.0",
        "typescript": ">=5.0.0"
    },
    "peerDependenciesMeta": {
        "eslint": {
            "optional": false
        }
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "eslint": "^8.53.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    }
}
